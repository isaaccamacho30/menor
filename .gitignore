 public static void main(String[] args) {
int[] arreglo = {10, 5, 8, 3, 2, 7};
        int menor = arreglo[0]; // Inicializar con el primer elemento del arreglo

        for (int i = 1; i < arreglo.length; i++) {
            if (arreglo[i] < menor) {
                menor = arreglo[i]; // Actualizar si se encuentra un elemento menor
            }
        }

        System.out.println("El menor elemento del arreglo es: " + menor);
    }
}

    


